{
  "defaults": {
    "loader": "fabric",
    "runMode": "client",
    "version": "1.21.7"
  },
  "gradleArgs": [
    "--build-cache",
    "--parallel",
    "-PmcVersion={mcVersion}",
    "-Ploader_cache_file={loader_cache_file}",
    "-PonlyLoader={loader}"
  ],
  "group": "uebliche/mcmeta",
  "id": "mod/midi",
  "kind": "project",
  "loaderSource": "mcmeta",
  "modrinth": {
    "projectId": "WGxKLoGM"
  },
  "name": "MIDI",
  "runModes": [
    "client",
    "server"
  ],
  "tasks": {
    "build": "build",
    "runClient": "runClient",
    "template": ":loader-{loader}:{task}"
  },
  "versionRange": {
    "min": "1.20.5"
  }
}
